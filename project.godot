; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gal"
run/main_scene="res://dialogue/styles/blue_archive/blue_archive.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Events="*res://examples/events.gd"
State="*res://examples/state.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Global="*res://Global.gd"

[dialogue_manager]

runtime/balloon_path="res://dialogue/styles/1/balloon.tscn"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[dotnet]

project/assembly_name="Gal"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[internationalization]

locale/translations_pot_files=PackedStringArray("res://examples/dialogue_with_input.dialogue", "res://examples/dialogue_for_point_n_click.dialogue", "res://examples/dialogue.dialogue", "res://examples/dialogue_for_csharp.dialogue", "res://examples/dialogue_for_visual_novel.dialogue", "res://dialogue/scenario/Abydos-1-1.dialogue", "res://dialogue/scenario/Abydos-1-test.dialogue", "res://dialogue/scenario/test.dialogue")

[rendering]

textures/vram_compression/import_etc2_astc=true
